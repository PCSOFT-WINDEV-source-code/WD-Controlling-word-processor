     code : |1+
      
      // Retrieves an instance
      gpclEditor = cEditor.EditorInstance()
      
      // Is the editor available?
      IF gpclEditor = Null THEN
      	Error(<§$0000§>)
      	EndProgram()
      END
      
      // depending on the editor, gray an image to indicate that the other editor is unavailable
      SWITCH gpclEditor..Class
      	CASE cMicrosoftWord:
      		IMG_OpenOffice = IMG_OpenOfficeGrayed	
      		IMG_OpenOffice..Grayed = True
      	CASE cOpenOfficeWriter:
      		IMG_MicrosoftWord = IMG_MicrosoftWordGrayed			
      		IMG_MicrosoftWord..Grayed=True
      END
      
