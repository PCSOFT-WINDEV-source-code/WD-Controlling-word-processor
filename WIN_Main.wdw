        internal_properties : CAAAAAgAAABHMVEpL2FBgmV2A1wo05YvLac2AJ77L1/dPCdqTJGJEdcaCNytVuVt9nVzigi228RN+FiYhr7jN8OSGUobNdGxuAwVQDxt9VdlD3ux+b+LF7b8ZWRFIF9elR1WQaJG9t38JBiGQZ8ZJ7eKu+A=
        code : |1-
         HourGlass(True)
         
         // If a document is already opened
         IF gpclEditor:IsOpened() THEN
         	SWITCH Confirm("A document is already opened.","Do you want to close it?")		
         		CASE Yes
         			// Close the document
         			gpclEditor:CloseDocument()
         		CASE No
         			// The opened document can no longer be used
         			Info("Any active document that is reopened has the read-only attribute")			
         		CASE Cancel
         			HourGlass(False)
         			RETURN
         	END
         END
         
         // Open a document
         IF gpclEditor:EditorOpenDocument() = False THEN
         	Error("Unable to open the document.")
         ELSE
         	WinStatus(WIN_Main,TopActive)
         END
         
         HourGlass(False)
